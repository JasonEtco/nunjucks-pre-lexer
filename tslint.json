{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-config-standard"
  ],
  "rulesDirectory": [
    "tslint-no-focused-test"
  ],
  "rules": {
    "no-focused-test": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "prefer-object-spread": false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "object-literal-sort-keys": false
  }
}
